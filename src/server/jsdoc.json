{
  "source": {
    "include": ["./src"],
    "exclude": ["./node_modules", "./tests", "./src/database/migrations"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}